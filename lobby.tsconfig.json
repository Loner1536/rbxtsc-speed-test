{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "dist/out/lobby.tsbuildinfo",
    "outDir": "dist/out/lobby",
    "rootDir": "sources/lobby/src",
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@rbxts",
      "node_modules/@flamework"
    ],
    "paths": {
      "@shared/*": [
        "sources/lobby/src/shared/*"
      ],
      "@server/*": [
        "sources/lobby/src/server/*"
      ],
      "@client/*": [
        "sources/lobby/src/client/*"
      ]
    },
    "plugins": [
      {
        "transform": "rbxts-transformer-flamework",
        "obfuscation": true
      }
    ]
  },
  "include": [
    "sources/lobby/src"
  ],
  "exclude": []
}
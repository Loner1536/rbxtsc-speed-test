function CloseTo_private:getExpectedType()
	return "number"
end

function CloseTo_private:toAsymmetricMatcher(): string
	return Array.join({
		self:toString(),
		self.sample,
		("(%s)"):format(tostring(pluralize("digit", self.precision))),
	}, " ")
end


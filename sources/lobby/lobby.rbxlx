			if state[key] == nil then
				connections[key] = nil
				disconnect()
			end
		end

		for key, value in next, state do
			if not connections[key] then
				connections[key] = factory(value, key) or noop
			end
		end
	end

	local unsubscribe = subscribe(callback, listener)

	store.peek(function(): ()
		listener(callback())
	end)


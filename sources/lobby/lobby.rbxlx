end

local blockSize = 64

type Queue = {
	new: () -> Queue,
	push: (self: Queue, value: any) -> (),
	shift: (self: Queue) -> any?,
}
type QueuePrivate = {
	new: () -> QueuePrivate,
	push: (self: QueuePrivate, value: any) -> (),
	shift: (self: QueuePrivate) -> any?,
